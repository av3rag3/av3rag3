target = input("? ")

nmapScan=input("nmap scan ? ")

if nmapScan == "y":
    nmap = subprocess.run(["nmap", "-p-", target], stdout=subprocess.PIPE, stderr=subprocess.PIPE, text=True)
    print(nmap.stdout)






