def nmap(ip):
	subprocess.run(f"sudo nmap -v -sC -sV -p- -oN nmap_enum {ip}", shell=True)
	return 

def dirb(ip):
	subprocess.run(f"dirb http://{ip} | tee dirb_enum", shell=True)
	return

def nikto(ip):
	subprocess.run(f"nikto -h {ip} | tee dirb_enum", shell=True)
	return


print(banner)

target = input("What's the ip address to enum ? ")

nmap(target)

port80 = input("?" )

if port80 == "y":
    dirb(target)
    nikto(target)
